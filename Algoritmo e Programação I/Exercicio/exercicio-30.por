programa
{
	funcao inicio()
	{
		inteiro pri=1,seu=1,ter
		escreva(pri,", ",seu,", ")
		para(inteiro i = 1; i<=20;i++){
			ter = pri + seu
			pri = seu
			seu =ter
			escreva(ter,", ")
			
		}
		
	}
}
