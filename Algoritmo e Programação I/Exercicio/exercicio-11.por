programa
{
	funcao inicio()
	{
		inteiro a

		escreva("Informe um numero: ")
		leia(a)

		se(a < 0){
			a = a * (-1)
			escreva (a)
		}senao{
			a = a
			escreva (a)
		}
	}
}
