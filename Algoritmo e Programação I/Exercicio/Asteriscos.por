programa
{
	funcao inicio()
	{
		inteiro num
		
		escreva("QTD de Linhas")
		leia(num)

	
		para(inteiro i = 0;i <= num; i++){
			
			para (inteiro c = 0; c < i;c++){
				escreva(" * ")
			}
			escreva("\n")
		}
	}
}
