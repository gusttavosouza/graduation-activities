programa
{
	funcao inicio()
	{
		inteiro numero,menor

		escreva("Informe um Numero: \n")
		leia (menor)


		para(inteiro i = 1;i<=19;i++){
			escreva("Informe um Numero: \n")
			leia (numero)
			se(numero < menor){
				menor = numero
			}

			
		}
		escreva("O menor numero digitado foi: ",menor)
		
	}
}
